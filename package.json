{
  "name": "sky-lark",
  "version": "0.0.10",
  "packageManager": "pnpm@6.27.2",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
    "pnpm": ">= 6",
    "npm": ">= 8"
  },
  "scripts": {
    "dev": "vite",
    "build:pro": "vite build --mode production",
    "build:pub": "vite build --mode pubproduction",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "format": "prettier . --write",
    "e2e": "cypress open",
    "e2e:ci": "cypress run",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint . --fix --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0  && pretty-quick --branch dev",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\"",
    "lint:style:fix": "stylelint --fix \"src/**/*.{css,scss,vue}\"",
    "cz": "cz",
    "commit": "git add . && cz-customizable",
    "release": "standard-version",
    "release:tags": "pnpm run release && git push --follow-tags origin main",
    "publish": "git push --follow-tags origin main",
    "vue-tsc": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm -y"
  },
  "config": {
    "commitizen": {
      "path": [
        "@commitlint/cz-commitlint",
        "cz-customizable"
      ]
    }
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "element-plus": "^1.3.0-beta.10",
    "postcss": "8.4.6",
    "vue": "^3.2.27",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.1.0",
    "@pnpm/types": "^7.9.0",
    "@sucrase/jest-plugin": "^2.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^2.1.0",
    "babel-core": "^6.26.3",
    "commitizen": "^4.2.4",
    "cypress": "^9.4.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.3.3",
    "markdownlint": "^0.25.1",
    "pnpm": "^6.29.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.49.7",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "sucrase": "^3.20.3",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.7.13",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.1",
    "vuex": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "cz-commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "*.{*.css,scss,vue}": "stylelint --fix"
  },
  "description": "This template should help get you started developing with Vue 3 and Typescript in Vite. The template uses Vue 3 `<script setup>` SFCs, check out the [script setup docs](https://v3.vuejs.org/api/sfc-script-setup.html#sfc-script-setup) to learn more.",
  "main": ".cz-config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taokepppooo/sky-lark.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taokepppooo/sky-lark/issues"
  },
  "homepage": "https://github.com/taokepppooo/sky-lark#readme"
}
